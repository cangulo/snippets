    <Header>
      <Title>Arrange Act Assert Snippet</Title>
      <Author>Carlos Angulo Mascarell</Author>
      <Description>Insert the words Arrange, Act and Assert in separate lines as comments</Description>
      <Shortcut>aaa</Shortcut>
    </Header>
    <Snippet>
      <Code Language="CSharp">
        <![CDATA[
        // Arrange
        $end$
        // Act
        // Assert
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
